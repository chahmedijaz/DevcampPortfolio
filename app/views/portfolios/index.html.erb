<h1>Portfolio Item </h1>

<%= link_to("Add New Portfolio", new_portfolio_path) %>

<% @portfolio_items.each do |portfolio_item|%>
    <p><%=portfolio_item.title%></p>
    <p><%=portfolio_item.subtitle%></p>
    <%= image_tag portfolio_item.image unless portfolio_item.image.nil? %>
    <%=link_to("Edit", edit_portfolio_path(portfolio_item.id))%>
<%end%>